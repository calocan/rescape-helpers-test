{
  "name": "rescape-helpers-test",
  "version": "0.0.118",
  "description": "Helpers for testing components",
  "main": "lib/index.js",
  "module": "src/index.js",
  "files": [
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calocan/rescape-helpers-test"
  },
  "author": "Andy Likuski andy@likuski.org",
  "bugs": {
    "url": "https://github.com/calocan/rescape-helpers-test"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "prepare": "yarn run install-peers -f && cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run lint && yarn run build",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive"
  },
  "dependencies": {
    "@apollo/client": "^3.2.5",
    "apollo-client": "^2.6.10",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-error": "^1.1.13",
    "apollo-link-schema": "^1.2.5",
    "apollo-test-utils": "^0.3.2",
    "core-js": "^3.6.5",
    "decamelize": "^4.0.0",
    "folktale": "^2.3.2",
    "graphql-tools": "^7.0.1",
    "longjohn": "^0.2.12",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-adopt": "^0.6.0",
    "react-apollo": "3.1.5",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "rebass": "^4.0.2",
    "redux": "^4.0.4",
    "regenerator-runtime": "^0.13.7",
    "render-props": "^1.1.0",
    "styled-components": "^5.2.0",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@apollo/react-hooks": "^4.0.0",
    "@apollo/react-testing": "^4.0.0",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@types/jest": "^26.0.15",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.2.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.1",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^7.0.2",
    "dotenv": "8.2.0",
    "enzyme": "^3.10.0",
    "enzyme-to-json": "^3.6.1",
    "enzyme-wait": "^1.0.9",
    "eslint": "^7.12.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jest": "^24.1.0",
    "graphql": ">=0.13.2",
    "install": "^0.13.0",
    "install-peers-cli": "^2.1.1",
    "jest": "26.6.1",
    "jest-cli": "^26.6.1",
    "jest-environment-enzyme": "^7.1.2",
    "jest-enzyme": "^7.1.0",
    "jsdoc-babel": "^0.5.0",
    "jsdom": "^16.4.0",
    "jsdom-global": "^3.0.2",
    "localstorage-polyfill": "^1.0.1",
    "react-dev-utils": "^11.0.0",
    "react-router-dom": "^5.2.0",
    "rollup": "^2.32.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-watch": "4.3.1"
  },
  "peerDependencies": {
    "graphql": ">=0.13.2",
    "ramda": ">=0.27.0",
    "rescape-apollo": ">=0.0.74",
    "rescape-helpers": ">=1.1.109",
    "rescape-helpers-component": ">=1.5.64",
    "rescape-log": ">=0.1.11",
    "rescape-place": ">=0.0.105",
    "rescape-ramda": ">=1.5.78",
    "rescape-sample-data": ">=1.2.60",
    "rescape-validate": ">=1.2.23"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme",
      "<rootDir>/config/jestsetup.js"
    ],
    "testEnvironment": "enzyme",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ],
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": true,
              "regenerator": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
