{
  "name": "rescape-helpers-test",
  "version": "0.0.34",
  "description": "Helpers for testing components",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "jsnext:main": "esm/index.js",
  "files": [
    "lib",
    "esm",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/calocan/rescape-helpers-test"
  },
  "author": "Andy Likuski andy@likuski.org",
  "bugs": {
    "url": "https://github.com/calocan/rescape-helpers-test"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "lint": "eslint src",
    "test": "cross-env jest",
    "test:watch": "yarn test -- --watch",
    "test:cov": "yarn test -- --coverage",
    "build": "rollup -c",
    "prepare": "yarn run install-peers -f && cross-env ENABLE_INTEGRATION_TESTS=false yarn run clean && yarn run lint && yarn run build",
    "build-publish-patch": "yarn run build && yarn run publish-patch",
    "build-publish-minor": "yarn run build && yarn run publish-minor",
    "publish-patch": "yarn version --patch && yarn publish --non-interactive",
    "publish-minor": "yarn version --minor && yarn publish --non-interactive"
  },
  "dependencies": {
    "apollo-client": "^2.6.4",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-error": "^1.1.11",
    "apollo-link-schema": "^1.2.3",
    "apollo-test-utils": "^0.3.2",
    "core-js": "^3.2.1",
    "decamelize": "^3.2.0",
    "folktale": "^2.3.2",
    "graphql-tools": "^4.0.5",
    "longjohn": "^0.2.12",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react-apollo": "3.0.1",
    "react-redux": "^7.1.0",
    "rebass": "^4.0.2",
    "redux": "^4.0.4",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.3",
    "util": "^0.12.1"
  },
  "devDependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "@apollo/react-testing": "^3.1.2",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@types/jest": "^24.0.17",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-es2015-modules-commonjs-simple": "^6.7.4",
    "babelrc-rollup": "^3.0.0",
    "cross-env": "^5.2.0",
    "dotenv": "8.0.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.4.0",
    "enzyme-wait": "^1.0.9",
    "eslint": "^6.1.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-jest": "^22.15.1",
    "graphql": ">=0.13.2",
    "install": "^0.13.0",
    "install-peers-cli": "^2.1.1",
    "jest": "24.9.0",
    "jest-cli": "^24.9.0",
    "jest-enzyme": "^7.1.0",
    "jsdoc-babel": "^0.5.0",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "localstorage-polyfill": "^1.0.1",
    "react": ">=16.4.1",
    "react-dev-utils": "^9.0.3",
    "react-dom": ">=16.4.1",
    "rescape-sample-data": "^1.2.60",
    "rollup": "^1.19.4",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "1.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "rollup-watch": "4.3.1"
  },
  "peerDependencies": {
    "graphql": ">=0.13.2",
    "react": ">=16.4.1",
    "react-dom": ">=16.4.1",
    "rescape-apollo": ">=0.0.74",
    "rescape-helpers-component": ">=1.5.64",
    "rescape-log": ">=0.1.11",
    "rescape-ramda": ">=1.5.78",
    "rescape-validate": ">=1.2.23"
  },
  "jest": {
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/config/jestsetup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "node"
    ]
  },
  "babel": {
    "sourceMaps": true,
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "corejs": "3x"
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "@babel/plugin-transform-runtime",
          [
            "transform-es2015-modules-commonjs-simple",
            {
              "noMangle": true
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "parser": "babel-eslint"
  }
}
